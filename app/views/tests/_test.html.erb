<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td>
    <ul>
      <% test.questions.each do |question| %>
        <li><%= question.content %></li>
      <% end %>
    </ul>
  </td>
  <td><%= link_to 'Просмотр', test_path(test) %></td>
  <td><%= link_to 'Изменить', edit_test_path(test) %></td>
  <td>
  <%= button_to 'Удалить', test, method: :delete, data: { confirm: 'Вы уверены?' } %>
  </td>
</tr>
